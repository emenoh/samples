package com.brilliantblue.panasonic.IFEdemo.personalMedia.usbMedia{	public class model extends com.brilliantblue.panasonic.IFEdemo.model	{		public static var POLL_INTERVAL:Number = 6;    	public static var MAX_FILES_PER_DIRECTORY:Number = 60;    	public var lastPoll:Number = 0;    	public var _directory:String;    	public var _isActive:Boolean;    	public var maxFilesPerDirectory:Number;    	public var pollInterval:Number;			public function model () 		{						        this._isActive = false;        this.pollInterval = POLL_INTERVAL;        this.maxFilesPerDirectory = MAX_FILES_PER_DIRECTORY;											}						public function setDirectory(directory)		{			this._directory = directory;		}			public function getDirectory()		{			return this._directory;		}			public function setIsActive(boolean)		{			this._isActive = boolean;		}			public function getIsActive()		{			return this._isActive;		}							}		}