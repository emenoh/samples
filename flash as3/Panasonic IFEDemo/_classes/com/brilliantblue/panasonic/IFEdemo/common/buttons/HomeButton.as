package com.brilliantblue.panasonic.IFEdemo.common.buttons{		import flash.display.*;	import flash.events.*;	import flash.events.MouseEvent;	import flash.geom.*;	import flash.utils.*;	public class HomeButton extends flash.display.SimpleButton	{		public var onMouseClickEvent;						public function HomeButton () 		{			this.addEventListener (MouseEvent.CLICK, onMouseClickEvent,false,0,true);		}			      }								public function MouseClickEvent(evt:MouseEvent)		{			trace("home pressed");		}	}