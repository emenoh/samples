package com.brilliantblue.panasonic.IFEdemo.common.buttons{		import caurina.transitions.*;	import flash.display.*;	import flash.text.*;	import flash.events.*;	import flash.geom.*;	import flash.utils.*;				public class PrevButton extends flash.display.MovieClip	{		public var onMouseClickEvent;		/*		public var Glow;		public var Fill;		public var doubleCheck;		*/		private var clickEvent;				public function PrevButton () 		{						this.x = 100;			this.y = 350;									this.addEventListener (MouseEvent.MOUSE_DOWN, onMouseDownEvent,false,0,true);			this.addEventListener (MouseEvent.MOUSE_OVER, onMouseDownEvent,false,0,true);			this.addEventListener (MouseEvent.MOUSE_UP, onMouseUpEvent,false,0,true);			this.addEventListener (MouseEvent.MOUSE_OUT, onMouseUpEvent,false,0,true);						//setTitle("SAMPLE TITLE");			setClickEvent(mouseClickEvent);					addEventListener(Event.REMOVED_FROM_STAGE, onRemoved,false,0,true);		}		/*		public function setTitle(buttonTitle){			this.buttonTitle = buttonTitle;			trace("testing");			this.btn_label.text = buttonTitle;					}		*/				private function onRemoved(e:Event){						this.removeEventListener (MouseEvent.MOUSE_DOWN, onMouseDownEvent);			this.removeEventListener (MouseEvent.MOUSE_OVER, onMouseDownEvent);			this.removeEventListener (MouseEvent.MOUSE_UP, onMouseUpEvent);			this.removeEventListener (MouseEvent.MOUSE_OUT, onMouseUpEvent);			this.removeEventListener (Event.REMOVED_FROM_STAGE, onRemoved);		}						public function setClickEvent(onMouseClickEvent){						this.addEventListener (MouseEvent.CLICK, onMouseClickEvent,false,0,true);					}								private function onMouseDownEvent(evt:MouseEvent)		{						Tweener.addTween(this, {alpha:1, time:1});			this.buttonMode = true;			this.mouseChildren = false;		}						private function onMouseUpEvent(evt:MouseEvent)		{						//Tweener.addTween(Glow, {alpha:0, time:2});			Tweener.addTween(this, {alpha:1, time:.2});						//doubleCheck = setInterval(Catinit, 1000);		}				public function mouseClickEvent(evt:MouseEvent)		{			//default event			trace("prev clicked");									}							}		}