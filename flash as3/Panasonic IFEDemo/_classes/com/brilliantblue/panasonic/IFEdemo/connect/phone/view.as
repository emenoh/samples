package com.brilliantblue.panasonic.IFEdemo.connect.phone{	import flash.display.*;	import flash.events.*;	import flash.text.*		import com.brilliantblue.panasonic.IFEdemo.view;	import com.brilliantblue.panasonic.IFEdemo.common.screen.*;	import com.brilliantblue.panasonic.IFEdemo.common.buttons.*;			public class view extends com.brilliantblue.panasonic.IFEdemo.view	{				public var myMovieClip:MovieClip;		public var myTextField:TextField;		public var myTextFormat:TextFormat;				public function view () 		{			setLayout("1column");			setScreenTitle("PHONE CONNECT");						myMovieClip = new MovieClip();			myTextFormat = new TextFormat();						myTextFormat.font = "Arial";			myTextFormat.color = 0xFFFFFF;			myTextFormat.size = 36;			myTextFormat.align = "center";						myTextField = new TextField();			myTextField.width = 900;			myTextField.height = 50;			myTextField.text = "Phone Connect MC goes here.";			myTextField.y = 160;			myTextField.setTextFormat(myTextFormat);			myMovieClip.addChild(myTextField);						myMovieClip.x = 130;			myMovieClip.y = 166;						this.addChild(myMovieClip);									//super.setContentMC(myMovieClip);								}							}		}